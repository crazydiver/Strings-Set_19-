var s:string;
    i,outp:integer;
begin
  readln(s);
  for i:=1 to Length(s) do
    if s[i]='a' then
      begin
        write('b');
        outp+=1;
      end
    else
      if s[i]='A' then
        begin
          write('B');
          outp+=1;
        end
    else
      if s[i]='b' then
      begin
        write('a');
        outp+=1;
      end
    else
      if s[i]='B' then
      begin
        write('A');
        outp+=1;
      end
    else
  write(s[i]);
  writeln;
  writeln(outp);
end.
